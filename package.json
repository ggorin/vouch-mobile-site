{
  "name": "vouch",
  "version": "1.0.0",
  "description": "Marketing website for Vouch Mobile, built with Eleventy.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "eleventy",
    "start": "eleventy --serve",
    "clean": "rm -rf _site",
    "validate:schema": "node scripts/validate-schema.js",
    "check:links": "linkinator _site --recurse --skip 'vouchmobile.com|kinsta|external' --format json --verbosity error",
    "build:validate": "npm run build && npm run validate:schema",
    "build:check": "npm run build && npm run check:links",
    "check:mobile": "node scripts/check-mobile-overflow.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggorin/vouch-mobile-site.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ggorin/vouch-mobile-site/issues"
  },
  "homepage": "https://github.com/ggorin/vouch-mobile-site#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^1.0.5",
    "@11ty/eleventy-plugin-bundle": "^3.0.7",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "eleventy-plugin-metagen": "^1.8.3",
    "glob": "^13.0.0",
    "html-minifier-terser": "^7.2.0",
    "inflight": "^1.0.6",
    "linkinator": "^7.5.3",
    "md-to-pdf": "^5.2.5",
    "rimraf": "^6.1.2"
  },
  "engines": {
    "node": "20.x"
  }
}
