---
layout: base.njk
---

{% block schema %}
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "LocalBusiness",
      "@id": "{{ site.url }}/#organization",
      "name": "{{ site.name }}",
      "url": "{{ site.url }}",
      "priceRange": "$30/month",
      "areaServed": {
        "@type": "State",
        "name": "{{ state.name }}",
        "containedInPlace": {
          "@type": "Country",
          "name": "United States"
        }
      }
    },
    {
      "@type": "Service",
      "serviceType": "Mobile Phone Service",
      "provider": {
        "@id": "{{ site.url }}/#organization"
      },
      "areaServed": {
        "@type": "State",
        "name": "{{ state.name }}",
        "containsPlace": [
          {% for city in state.cities %}
          {"@type": "City", "name": "{{ city.name }}"}{% if not loop.last %},{% endif %}
          {% endfor %}
        ]
      }
    },
    {
      "@type": "FAQPage",
      "mainEntity": [
        {% if state.faqs %}
        {% for faq in state.faqs %}
        {
          "@type": "Question",
          "name": "{{ faq.question }}",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "{{ faq.answer }}"
          }
        }{% if not loop.last %},{% endif %}
        {% endfor %}
        {% else %}
        {
          "@type": "Question",
          "name": "Does Vouch Mobile work in {{ state.name }}?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Yes! Vouch Mobile uses AT&T's network which provides excellent coverage across {{ state.name }} with {{ state.coverage }} population coverage."
          }
        },
        {
          "@type": "Question",
          "name": "Does Vouch Mobile require a credit check in {{ state.name }}?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "No! Vouch Mobile never requires a credit check. {{ state.name }} residents can sign up regardless of credit history."
          }
        },
        {
          "@type": "Question",
          "name": "Does Vouch Mobile have 5G in {{ state.name }}?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Yes! 5G coverage is available in major {{ state.name }} cities including {{ state.cities[0].name }}."
          }
        }
        {% endif %}
      ]
    },
    {
      "@type": "BreadcrumbList",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "Home",
          "item": "{{ site.url }}"
        },
        {
          "@type": "ListItem",
          "position": 2,
          "name": "Coverage",
          "item": "{{ site.url }}/coverage/"
        },
        {
          "@type": "ListItem",
          "position": 3,
          "name": "{{ state.name }}"
        }
      ]
    }
  ]
}
</script>
{% endblock %}

<!-- State Hero V2 -->
<section class="state-hero-v2">
  <div class="state-hero-bg">
    {% if state.hasImage %}
    {% set imgPath = "/images/coverage/" + state.slug + "." + (state.imageExtension | default('jpg')) %}
    {% set imgAlt = state.name + " coverage" %}
    {% image imgPath, imgAlt, [600, 1200, 1920], "100vw", {class: "state-hero-image"} %}
    {% endif %}
    <div class="state-hero-overlay"></div>
    <div class="state-hero-grid"></div>
  </div>

  <div class="container">
    <nav class="breadcrumb-v2" aria-label="Breadcrumb">
      <a href="/">Home</a>
      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="9 18 15 12 9 6"></polyline></svg>
      <a href="/coverage/">Coverage</a>
      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="9 18 15 12 9 6"></polyline></svg>
      <span>{{ state.name }}</span>
    </nav>

    <div class="state-hero-content">
      <div class="state-hero-badge">
        <svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polygon points="13 2 3 14 12 14 11 22 21 10 12 10 13 2"></polygon></svg>
        {{ state.coverage }} Coverage
      </div>
      <h1 class="state-hero-title">
        <span class="title-small">Vouch Mobile in</span>
        <span class="title-state">{{ state.name }}</span>
      </h1>
      <p class="state-hero-subtitle">Unlimited plans on AT&T's network. No credit check required.</p>

      <div class="state-hero-actions">
        <a href="https://vouchmobile.com" class="btn btn-primary btn-lg">
          Get Started
          <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
        </a>
        <a href="#faq" class="btn btn-outline-light btn-lg">View FAQ</a>
      </div>
    </div>
  </div>
</section>

<!-- Savings Highlight -->
{% if state.annualSavings %}
<section class="savings-banner">
  <div class="container">
    <div class="savings-content">
      <div class="savings-icon">
        <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><circle cx="12" cy="12" r="10"></circle><path d="M16 8h-6a2 2 0 1 0 0 4h4a2 2 0 1 1 0 4H8"></path><path d="M12 18V6"></path></svg>
      </div>
      <div class="savings-text">
        <span class="savings-label">{{ state.name }} residents save up to</span>
        <span class="savings-amount">{{ state.annualSavings }}/year</span>
        <span class="savings-detail">compared to the average {{ state.avgPhoneBill }}/mo phone bill</span>
      </div>
      <a href="/plans/" class="btn btn-primary">See Plans</a>
    </div>
  </div>
</section>
{% endif %}

<!-- Key Facts Section -->
<section class="section state-facts-section">
  <div class="container">
    <div class="state-layout">
      <div class="state-main">
        <!-- Quick Stats -->
        <div class="quick-stats-grid">
          <div class="quick-stat">
            <div class="quick-stat-icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path><circle cx="9" cy="7" r="4"></circle><path d="M23 21v-2a4 4 0 0 0-3-3.87"></path><path d="M16 3.13a4 4 0 0 1 0 7.75"></path></svg>
            </div>
            <div class="quick-stat-content">
              <span class="quick-stat-value">{{ state.population | default('Millions') }}</span>
              <span class="quick-stat-label">Population</span>
            </div>
          </div>
          <div class="quick-stat">
            <div class="quick-stat-icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M5 12.55a11 11 0 0 1 14.08 0"></path><path d="M1.42 9a16 16 0 0 1 21.16 0"></path><path d="M8.53 16.11a6 6 0 0 1 6.95 0"></path><line x1="12" y1="20" x2="12.01" y2="20"></line></svg>
            </div>
            <div class="quick-stat-content">
              <span class="quick-stat-value">{{ state.coverage }}</span>
              <span class="quick-stat-label">Coverage</span>
            </div>
          </div>
          <div class="quick-stat highlight">
            <div class="quick-stat-icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="1" x2="12" y2="23"></line><path d="M17 5H9.5a3.5 3.5 0 0 0 0 7h5a3.5 3.5 0 0 1 0 7H6"></path></svg>
            </div>
            <div class="quick-stat-content">
              <span class="quick-stat-value">$30/mo</span>
              <span class="quick-stat-label">Vouch Price</span>
            </div>
          </div>
          <div class="quick-stat">
            <div class="quick-stat-icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M22 11.08V12a10 10 0 1 1-5.93-9.14"></path><polyline points="22 4 12 14.01 9 11.01"></polyline></svg>
            </div>
            <div class="quick-stat-content">
              <span class="quick-stat-value">No</span>
              <span class="quick-stat-label">Credit Check</span>
            </div>
          </div>
        </div>

        <!-- Unique Hook -->
        {% if state.uniqueHook %}
        <div class="state-hook-card">
          <div class="hook-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><circle cx="12" cy="12" r="10"></circle><line x1="12" y1="16" x2="12" y2="12"></line><line x1="12" y1="8" x2="12.01" y2="8"></line></svg>
          </div>
          <p>{{ state.uniqueHook }}</p>
        </div>
        {% endif %}

        <!-- Network Coverage Section -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">Network</span>
            <h2>AT&T Coverage in {{ state.name }}</h2>
          </div>
          <p>Vouch Mobile operates on AT&T's award-winning network, providing {{ state.name }} residents with reliable 5G and LTE coverage across the {{ state.nickname }}. With <strong>{{ state.coverage }} population coverage</strong>, you'll stay connected wherever you go.</p>

          {% if state.externalLinks %}
          <p>According to the <a href="{{ state.externalLinks[1].url }}" target="_blank" rel="noopener">{{ state.externalLinks[1].text }}</a>, AT&T provides extensive wireless coverage throughout {{ state.name }}, including urban centers and many rural areas.</p>
          {% endif %}
        </div>

        <!-- Cities Coverage -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">Cities</span>
            <h2>{{ state.name }} Cities We Cover</h2>
          </div>
          <div class="cities-grid-v2">
            {% for city in state.cities %}
            <div class="city-card-v2">
              <div class="city-icon">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"></path><circle cx="12" cy="10" r="3"></circle></svg>
              </div>
              <div class="city-content">
                <h3>{{ city.name }}</h3>
                <p>{{ city.description }}</p>
              </div>
              <div class="city-badge">
                <span class="status-dot"></span>
                5G
              </div>
            </div>
            {% endfor %}
          </div>
        </div>

        <!-- Why Vouch Section -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">Benefits</span>
            <h2>Why {{ state.name }} Residents Choose Vouch</h2>
          </div>
          <div class="benefits-grid-v2">
            {% if state.whyVouch %}
            {% for reason in state.whyVouch %}
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p>{{ reason }}</p>
            </div>
            {% endfor %}
            {% else %}
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>No Credit Check:</strong> Perfect for new {{ state.name }} residents, students, or anyone rebuilding credit.</p>
            </div>
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>AT&T Network Quality:</strong> Get reliable coverage at a fraction of the price.</p>
            </div>
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>Simple Pricing:</strong> Just $30/month for unlimited everything. No hidden fees.</p>
            </div>
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>5G Included:</strong> Access to AT&T's 5G network across major {{ state.name }} cities.</p>
            </div>
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>No Contracts:</strong> Month-to-month service. Cancel anytime.</p>
            </div>
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>Keep Your Number:</strong> Easily port your existing phone number.</p>
            </div>
            {% endif %}
          </div>
        </div>

        <!-- Getting Started Steps -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">How To</span>
            <h2>Get Started in {{ state.name }}</h2>
          </div>
          <div class="steps-grid-v2">
            <div class="step-card-v2">
              <div class="step-number">1</div>
              <h3>Check Your Phone</h3>
              <p>Make sure your phone is unlocked and compatible.</p>
              <a href="/guides/check-phone-unlocked/" class="step-link">Learn how →</a>
            </div>
            <div class="step-card-v2">
              <div class="step-number">2</div>
              <h3>Choose Your Plan</h3>
              <p>Select our $30/month unlimited plan.</p>
            </div>
            <div class="step-card-v2">
              <div class="step-number">3</div>
              <h3>Order Your SIM</h3>
              <p>Free shipping to any {{ state.name }} address.</p>
            </div>
            <div class="step-card-v2">
              <div class="step-number">4</div>
              <h3>Activate & Save</h3>
              <p>Simple activation takes just minutes.</p>
              <a href="/guides/how-to-switch-carriers/" class="step-link">Switching guide →</a>
            </div>
          </div>
        </div>

        <!-- Related Resources -->
        <div class="content-section">
          <div class="related-links-v2">
            <h3>Related Resources</h3>
            <div class="links-grid">
              <a href="/guides/no-credit-check-phone-plans/" class="related-link-card">
                <span class="link-title">Complete Guide to No Credit Check Phone Plans</span>
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
              </a>
              <a href="/guides/how-to-switch-carriers/" class="related-link-card">
                <span class="link-title">How to Switch Phone Carriers</span>
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
              </a>
              <a href="/compare/" class="related-link-card">
                <span class="link-title">Compare Vouch to Other Carriers</span>
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
              </a>
            </div>
          </div>
        </div>
      </div>

      <!-- Sidebar -->
      <aside class="state-sidebar">
        <div class="sidebar-plan-card-v2">
          <div class="plan-header">
            <span class="plan-tag">{{ state.name }} Plan</span>
            <div class="plan-price">
              <span class="price-amount">$30</span>
              <span class="price-period">/month</span>
            </div>
          </div>
          <ul class="plan-features-v2">
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              Unlimited Talk
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              Unlimited Text
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              Unlimited Data
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              5G Access
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              Mobile Hotspot
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              No Credit Check
            </li>
          </ul>
          <a href="https://vouchmobile.com" class="btn btn-primary btn-block">Get Started</a>
          <p class="plan-note">Free shipping to {{ state.name }}</p>
        </div>

        <div class="sidebar-states-card">
          <h3>Other States</h3>
          <ul class="other-states-list">
            {% for s in states | slice(0, 5) %}
            {% if s.slug != state.slug %}
            <li>
              <a href="/coverage/{{ s.slug }}/">
                <span>{{ s.name }}</span>
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="9 18 15 12 9 6"></polyline></svg>
              </a>
            </li>
            {% endif %}
            {% endfor %}
          </ul>
          <a href="/coverage/" class="view-all-link">View All States →</a>
        </div>
      </aside>
    </div>
  </div>
</section>

<!-- FAQ Section -->
<section class="section section-alt state-faq-section" id="faq">
  <div class="container">
    <div class="section-header-v2 text-center">
      <span class="section-tag">FAQ</span>
      <h2 class="section-title-v2">{{ state.name }} Coverage Questions</h2>
      <p class="section-subtitle-v2">Everything you need to know about Vouch Mobile in {{ state.name }}</p>
    </div>

    <div class="faq-grid-v2">
      {% if state.faqs %}
      {% for faq in state.faqs %}
      <div class="faq-card-v2">
        <button class="faq-trigger" aria-expanded="false">
          <span class="faq-question-text">{{ faq.question }}</span>
          <span class="faq-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="5" x2="12" y2="19"></line><line x1="5" y1="12" x2="19" y2="12"></line></svg>
          </span>
        </button>
        <div class="faq-item-content">
          <p>{{ faq.answer }}</p>
        </div>
      </div>
      {% endfor %}
      {% else %}
      <div class="faq-card-v2">
        <button class="faq-trigger" aria-expanded="false">
          <span class="faq-question-text">Does Vouch Mobile work in {{ state.name }}?</span>
          <span class="faq-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="5" x2="12" y2="19"></line><line x1="5" y1="12" x2="19" y2="12"></line></svg>
          </span>
        </button>
        <div class="faq-item-content">
          <p>Yes! Vouch Mobile uses AT&T's network which provides excellent coverage across {{ state.name }} with {{ state.coverage }} population coverage.</p>
        </div>
      </div>
      <div class="faq-card-v2">
        <button class="faq-trigger" aria-expanded="false">
          <span class="faq-question-text">Does Vouch Mobile require a credit check in {{ state.name }}?</span>
          <span class="faq-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="5" x2="12" y2="19"></line><line x1="5" y1="12" x2="19" y2="12"></line></svg>
          </span>
        </button>
        <div class="faq-item-content">
          <p>No! Vouch Mobile never requires a credit check. {{ state.name }} residents can sign up regardless of credit history.</p>
        </div>
      </div>
      <div class="faq-card-v2">
        <button class="faq-trigger" aria-expanded="false">
          <span class="faq-question-text">Does Vouch Mobile have 5G in {{ state.name }}?</span>
          <span class="faq-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="5" x2="12" y2="19"></line><line x1="5" y1="12" x2="19" y2="12"></line></svg>
          </span>
        </button>
        <div class="faq-item-content">
          <p>Yes! 5G coverage is available in major {{ state.name }} cities including {{ state.cities[0].name }}{% if state.cities[1] %}, {{ state.cities[1].name }}{% endif %}{% if state.cities[2] %}, and {{ state.cities[2].name }}{% endif %}.</p>
        </div>
      </div>
      {% endif %}
    </div>
  </div>
</section>

<!-- State CTA Section -->
<section class="section state-cta-section">
  <div class="container">
    <div class="state-cta-wrapper">
      <div class="cta-glow"></div>
      <div class="state-cta-content">
