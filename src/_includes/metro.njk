---
layout: base.njk
---

{% block schema %}
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "LocalBusiness",
      "@id": "{{ site.url }}/#organization",
      "name": "{{ site.name }}",
      "url": "{{ site.url }}",
      "priceRange": "$30/month",
      "areaServed": {
        "@type": "City",
        "name": "{{ metro.name }}",
        "containedInPlace": {
          "@type": "State",
          "name": "{{ metro.stateName }}",
          "containedInPlace": {
            "@type": "Country",
            "name": "United States"
          }
        }
      }
    },
    {
      "@type": "Service",
      "serviceType": "Mobile Phone Service",
      "provider": {
        "@id": "{{ site.url }}/#organization"
      },
      "areaServed": {
        "@type": "City",
        "name": "{{ metro.name }}",
        "containsPlace": [
          {% for area in metro.areas %}
          {"@type": "Place", "name": "{{ area.name }}"}{% if not loop.last %},{% endif %}
          {% endfor %}
        ]
      }
    },
    {
      "@type": "FAQPage",
      "mainEntity": [
        {% for faq in metro.faqs %}
        {
          "@type": "Question",
          "name": "{{ faq.question }}",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "{{ faq.answer }}"
          }
        }{% if not loop.last %},{% endif %}
        {% endfor %}
      ]
    },
    {
      "@type": "BreadcrumbList",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "Home",
          "item": "{{ site.url }}"
        },
        {
          "@type": "ListItem",
          "position": 2,
          "name": "Coverage",
          "item": "{{ site.url }}/coverage/"
        },
        {
          "@type": "ListItem",
          "position": 3,
          "name": "{{ metro.stateName }}",
          "item": "{{ site.url }}/coverage/{{ metro.state }}/"
        },
        {
          "@type": "ListItem",
          "position": 4,
          "name": "{{ metro.name }}"
        }
      ]
    }
  ]
}
</script>
{% endblock %}

<!-- Metro Hero V2 -->
<section class="state-hero-v2 metro-hero">
  <div class="state-hero-bg">
    <div class="state-hero-overlay"></div>
    <div class="state-hero-grid"></div>
  </div>

  <div class="container">
    <nav class="breadcrumb-v2" aria-label="Breadcrumb">
      <a href="/">Home</a>
      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="9 18 15 12 9 6"></polyline></svg>
      <a href="/coverage/">Coverage</a>
      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="9 18 15 12 9 6"></polyline></svg>
      <a href="/coverage/{{ metro.state }}/">{{ metro.stateName }}</a>
      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="9 18 15 12 9 6"></polyline></svg>
      <span>{{ metro.name }}</span>
    </nav>

    <div class="state-hero-content">
      <div class="state-hero-badge">
        <svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polygon points="13 2 3 14 12 14 11 22 21 10 12 10 13 2"></polygon></svg>
        {{ metro.coverage }} Coverage
      </div>
      <h1 class="state-hero-title">
        <span class="title-small">Vouch Mobile in</span>
        <span class="title-state">{{ metro.name }}</span>
      </h1>
      <p class="state-hero-subtitle">Unlimited plans on AT&T's network. No credit check required.</p>

      <div class="state-hero-actions">
        <a href="https://vouchmobile.com" class="btn btn-primary btn-lg">
          Get Started
          <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
        </a>
        <a href="#faq" class="btn btn-outline-light btn-lg">View FAQ</a>
      </div>
    </div>
  </div>
</section>

<!-- Savings Highlight -->
{% if metro.annualSavings %}
<section class="savings-banner">
  <div class="container">
    <div class="savings-content">
      <div class="savings-icon">
        <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><circle cx="12" cy="12" r="10"></circle><path d="M16 8h-6a2 2 0 1 0 0 4h4a2 2 0 1 1 0 4H8"></path><path d="M12 18V6"></path></svg>
      </div>
      <div class="savings-text">
        <span class="savings-label">{{ metro.name }} residents save up to</span>
        <span class="savings-amount">{{ metro.annualSavings }}/year</span>
        <span class="savings-detail">compared to the average {{ metro.avgPhoneBill }}/mo phone bill</span>
      </div>
      <a href="/plans/" class="btn btn-primary">See Plans</a>
    </div>
  </div>
</section>
{% endif %}

<!-- Key Facts Section -->
<section class="section state-facts-section">
  <div class="container">
    <div class="state-layout">
      <div class="state-main">
        <!-- Quick Stats -->
        <div class="quick-stats-grid">
          <div class="quick-stat">
            <div class="quick-stat-icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path><circle cx="9" cy="7" r="4"></circle><path d="M23 21v-2a4 4 0 0 0-3-3.87"></path><path d="M16 3.13a4 4 0 0 1 0 7.75"></path></svg>
            </div>
            <div class="quick-stat-content">
              <span class="quick-stat-value">{{ metro.metroPopulation }}</span>
              <span class="quick-stat-label">Metro Population</span>
            </div>
          </div>
          <div class="quick-stat">
            <div class="quick-stat-icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M5 12.55a11 11 0 0 1 14.08 0"></path><path d="M1.42 9a16 16 0 0 1 21.16 0"></path><path d="M8.53 16.11a6 6 0 0 1 6.95 0"></path><line x1="12" y1="20" x2="12.01" y2="20"></line></svg>
            </div>
            <div class="quick-stat-content">
              <span class="quick-stat-value">{{ metro.coverage }}</span>
              <span class="quick-stat-label">Coverage</span>
            </div>
          </div>
          <div class="quick-stat highlight">
            <div class="quick-stat-icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="1" x2="12" y2="23"></line><path d="M17 5H9.5a3.5 3.5 0 0 0 0 7h5a3.5 3.5 0 0 1 0 7H6"></path></svg>
            </div>
            <div class="quick-stat-content">
              <span class="quick-stat-value">$30/mo</span>
              <span class="quick-stat-label">Vouch Price</span>
            </div>
          </div>
          <div class="quick-stat">
            <div class="quick-stat-icon">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M22 11.08V12a10 10 0 1 1-5.93-9.14"></path><polyline points="22 4 12 14.01 9 11.01"></polyline></svg>
            </div>
            <div class="quick-stat-content">
              <span class="quick-stat-value">No</span>
              <span class="quick-stat-label">Credit Check</span>
            </div>
          </div>
        </div>

        <!-- Unique Hook -->
        {% if metro.uniqueHook %}
        <div class="state-hook-card">
          <div class="hook-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><circle cx="12" cy="12" r="10"></circle><line x1="12" y1="16" x2="12" y2="12"></line><line x1="12" y1="8" x2="12.01" y2="8"></line></svg>
          </div>
          <p>{{ metro.uniqueHook }}</p>
        </div>
        {% endif %}

        <!-- Network Coverage Section -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">Network</span>
            <h2>AT&T Coverage in {{ metro.name }}</h2>
          </div>
          <p>Vouch Mobile operates on AT&T's award-winning network, providing {{ metro.name }} residents with reliable 5G and LTE coverage across the metro area. With <strong>{{ metro.coverage }} population coverage</strong> and average speeds of {{ metro.networkSpeed }}, you'll stay connected wherever you go in the {{ metro.name }} area.</p>
          <p>As part of the {{ metro.metroPopulation }} population metro, {{ metro.name }} benefits from AT&T's ongoing network investment and 5G expansion.</p>
        </div>

        <!-- Areas Coverage -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">Areas</span>
            <h2>{{ metro.name }} Neighborhoods We Cover</h2>
          </div>
          <div class="cities-grid-v2">
            {% for area in metro.areas %}
            <div class="city-card-v2">
              <div class="city-icon">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"></path><circle cx="12" cy="10" r="3"></circle></svg>
              </div>
              <div class="city-content">
                <h3>{{ area.name }}</h3>
                <p>{{ area.description }}</p>
              </div>
              <div class="city-badge">
                <span class="status-dot"></span>
                5G
              </div>
            </div>
            {% endfor %}
          </div>
        </div>

        <!-- Why Vouch Section -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">Benefits</span>
            <h2>Why {{ metro.name }} Residents Choose Vouch</h2>
          </div>
          <div class="benefits-grid-v2">
            {% if metro.whyVouch %}
            {% for reason in metro.whyVouch %}
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p>{{ reason }}</p>
            </div>
            {% endfor %}
            {% else %}
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>No Credit Check:</strong> Perfect for new {{ metro.name }} residents, students, or anyone rebuilding credit.</p>
            </div>
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>AT&T Network Quality:</strong> Get reliable coverage at a fraction of the price.</p>
            </div>
            <div class="benefit-item-v2">
              <div class="benefit-check">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              </div>
              <p><strong>Simple Pricing:</strong> Just $30/month for unlimited everything. No hidden fees.</p>
            </div>
            {% endif %}
          </div>
        </div>

        <!-- Local Demographics Section -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">Who Benefits</span>
            <h2>Vouch Mobile for {{ metro.name }} Locals</h2>
          </div>
          <div class="demographics-grid">
            {% for angle in metro.localAngles %}
            <div class="demographic-card">
              <div class="demographic-icon">
                {% if angle.icon == "car" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M14 16H9m10 0h3v-3.15a1 1 0 0 0-.84-.99L16 11l-2.7-3.6a1 1 0 0 0-.8-.4H5.24a2 2 0 0 0-1.8 1.1l-.8 1.63A6 6 0 0 0 2 12.42V16h2"></path><circle cx="6.5" cy="16.5" r="2.5"></circle><circle cx="16.5" cy="16.5" r="2.5"></circle></svg>
                {% elif angle.icon == "train" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="4" y="3" width="16" height="16" rx="2"></rect><path d="M4 11h16"></path><path d="M12 3v8"></path><path d="m8 19-2 3"></path><path d="m18 22-2-3"></path><path d="M8 15h0"></path><path d="M16 15h0"></path></svg>
                {% elif angle.icon == "film" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="2" y="2" width="20" height="20" rx="2.18" ry="2.18"></rect><line x1="7" y1="2" x2="7" y2="22"></line><line x1="17" y1="2" x2="17" y2="22"></line><line x1="2" y1="12" x2="22" y2="12"></line><line x1="2" y1="7" x2="7" y2="7"></line><line x1="2" y1="17" x2="7" y2="17"></line><line x1="17" y1="17" x2="22" y2="17"></line><line x1="17" y1="7" x2="22" y2="7"></line></svg>
                {% elif angle.icon == "briefcase" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="2" y="7" width="20" height="14" rx="2" ry="2"></rect><path d="M16 21V5a2 2 0 0 0-2-2h-4a2 2 0 0 0-2 2v16"></path></svg>
                {% elif angle.icon == "code" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="16 18 22 12 16 6"></polyline><polyline points="8 6 2 12 8 18"></polyline></svg>
                {% elif angle.icon == "chart" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="12" y1="20" x2="12" y2="10"></line><line x1="18" y1="20" x2="18" y2="4"></line><line x1="6" y1="20" x2="6" y2="16"></line></svg>
                {% elif angle.icon == "coffee" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M17 8h1a4 4 0 1 1 0 8h-1"></path><path d="M3 8h14v9a4 4 0 0 1-4 4H7a4 4 0 0 1-4-4Z"></path><line x1="6" y1="2" x2="6" y2="4"></line><line x1="10" y1="2" x2="10" y2="4"></line><line x1="14" y1="2" x2="14" y2="4"></line></svg>
                {% elif angle.icon == "graduation" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M22 10v6M2 10l10-5 10 5-10 5z"></path><path d="M6 12v5c3 3 9 3 12 0v-5"></path></svg>
                {% elif angle.icon == "medical" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M22 12h-4l-3 9L9 3l-3 9H2"></path></svg>
                {% elif angle.icon == "lightning" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polygon points="13 2 3 14 12 14 11 22 21 10 12 10 13 2"></polygon></svg>
                {% elif angle.icon == "home" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="m3 9 9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z"></path><polyline points="9 22 9 12 15 12 15 22"></polyline></svg>
                {% elif angle.icon == "sun" %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><circle cx="12" cy="12" r="5"></circle><line x1="12" y1="1" x2="12" y2="3"></line><line x1="12" y1="21" x2="12" y2="23"></line><line x1="4.22" y1="4.22" x2="5.64" y2="5.64"></line><line x1="18.36" y1="18.36" x2="19.78" y2="19.78"></line><line x1="1" y1="12" x2="3" y2="12"></line><line x1="21" y1="12" x2="23" y2="12"></line><line x1="4.22" y1="19.78" x2="5.64" y2="18.36"></line><line x1="18.36" y1="5.64" x2="19.78" y2="4.22"></line></svg>
                {% else %}
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path><circle cx="9" cy="7" r="4"></circle><path d="M23 21v-2a4 4 0 0 0-3-3.87"></path><path d="M16 3.13a4 4 0 0 1 0 7.75"></path></svg>
                {% endif %}
              </div>
              <div class="demographic-content">
                <h3>{{ angle.title }}</h3>
                <p>{{ angle.description }}</p>
              </div>
            </div>
            {% endfor %}
          </div>
        </div>

        <!-- Getting Started Steps -->
        <div class="content-section">
          <div class="section-header-inline">
            <span class="section-tag">How To</span>
            <h2>Get Started in {{ metro.name }}</h2>
          </div>
          <div class="steps-grid-v2">
            <div class="step-card-v2">
              <div class="step-number">1</div>
              <h3>Check Your Phone</h3>
              <p>Make sure your phone is unlocked and compatible.</p>
              <a href="/guides/check-phone-unlocked/" class="step-link">Learn how &rarr;</a>
            </div>
            <div class="step-card-v2">
              <div class="step-number">2</div>
              <h3>Choose Your Plan</h3>
              <p>Select our $30/month unlimited plan.</p>
            </div>
            <div class="step-card-v2">
              <div class="step-number">3</div>
              <h3>Order Your SIM</h3>
              <p>Free shipping to any {{ metro.name }} address.</p>
            </div>
            <div class="step-card-v2">
              <div class="step-number">4</div>
              <h3>Activate & Save</h3>
              <p>Simple activation takes just minutes.</p>
              <a href="/guides/how-to-switch-carriers/" class="step-link">Switching guide &rarr;</a>
            </div>
          </div>
        </div>

        <!-- Related Resources -->
        <div class="content-section">
          <div class="related-links-v2">
            <h3>Related Resources</h3>
            <div class="links-grid">
              <a href="/coverage/{{ metro.state }}/" class="related-link-card">
                <span class="link-title">{{ metro.stateName }} Statewide Coverage</span>
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
              </a>
              <a href="/guides/no-credit-check-phone-plans/" class="related-link-card">
                <span class="link-title">Guide to No Credit Check Phone Plans</span>
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
              </a>
              <a href="/compare/" class="related-link-card">
                <span class="link-title">Compare Vouch to Other Carriers</span>
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
              </a>
            </div>
          </div>
        </div>
      </div>

      <!-- Sidebar -->
      <aside class="state-sidebar">
        <div class="sidebar-plan-card-v2">
          <div class="plan-header">
            <span class="plan-tag">{{ metro.name }} Plan</span>
            <div class="plan-price">
              <span class="price-amount">$30</span>
              <span class="price-period">/month</span>
            </div>
          </div>
          <ul class="plan-features-v2">
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              Unlimited Talk
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              Unlimited Text
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              Unlimited Data
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              5G Access
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              Mobile Hotspot
            </li>
            <li>
              <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2.5"><polyline points="20 6 9 17 4 12"></polyline></svg>
              No Credit Check
            </li>
          </ul>
          <a href="https://vouchmobile.com" class="btn btn-primary btn-block">Get Started</a>
          <p class="plan-note">Free shipping to {{ metro.name }}</p>
        </div>

        <div class="sidebar-states-card">
          <h3>More Metro Areas</h3>
          <ul class="other-states-list">
