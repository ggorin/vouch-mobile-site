</section>

<!-- CTA -->
<section class="plans-cta">
  <div class="plans-cta__bg">
    <div class="plans-cta__grid"></div>
    <div class="plans-cta__glow"></div>
  </div>

  <div class="container">
    <div class="plans-cta__content">
      <h2 class="plans-cta__title">Ready to Save?</h2>
      <p class="plans-cta__text">Switch to Vouch Mobile in minutes. No credit check, no contracts, no surprises.</p>

      <div class="plans-cta__buttons">
        <a href="https://vouchmobile.com" class="btn btn-white btn-lg">
          Get Started
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M5 12h14M12 5l7 7-7 7"/>
          </svg>
        </a>
        <a href="/coverage/" class="btn btn-outline-light btn-lg">Check Coverage</a>
      </div>

      <p class="plans-cta__note">30-day money-back guarantee</p>
    </div>
  </div>
</section>

<script>
// Billing toggle functionality
document.addEventListener('DOMContentLoaded', function() {
  const toggle = document.getElementById('billing-toggle');
  const amounts = document.querySelectorAll('.plan-card__amount');
  const annualNotes = document.querySelectorAll('.annual-note');
  const monthlyNotes = document.querySelectorAll('.monthly-note');
  const toggleLabels = document.querySelectorAll('.plans-toggle__label');

  function updatePricing() {
    const isAnnual = toggle.checked;

    amounts.forEach(amount => {
      const price = isAnnual ? amount.dataset.annual : amount.dataset.monthly;
      amount.textContent = price;
    });

    annualNotes.forEach(note => {
      note.style.display = isAnnual ? 'block' : 'none';
    });

    monthlyNotes.forEach(note => {
      note.style.display = isAnnual ? 'none' : 'block';
    });

    toggleLabels.forEach(label => {
      if (label.dataset.billing === 'annual') {
        label.classList.toggle('plans-toggle__label--active', isAnnual);
      } else {
        label.classList.toggle('plans-toggle__label--active', !isAnnual);
      }
    });
  }

  toggle.addEventListener('change', updatePricing);

  // FAQ accordion
  const faqQuestions = document.querySelectorAll('.plans-faq__question');

  faqQuestions.forEach(question => {
    question.addEventListener('click', function() {
      const isExpanded = this.getAttribute('aria-expanded') === 'true';

      // Close all others
      faqQuestions.forEach(q => {
        q.setAttribute('aria-expanded', 'false');
        q.closest('.plans-faq__item').classList.remove('plans-faq__item--open');
      });

      // Toggle current
      if (!isExpanded) {
        this.setAttribute('aria-expanded', 'true');
        this.closest('.plans-faq__item').classList.add('plans-faq__item--open');
      }
    });
  });
});
</script>
