---
layout: base.njk
title: Plans & Pricing
description: Vouch Mobile offers unlimited talk, text, and data for just $30/month. No credit check, no contracts. See our simple pricing.
eleventyNavigation:
  key: Plans
  order: 2
---

{% block schema %}
<script type="application/ld+json">
{{ {
  "@context": "https://schema.org",
  "@type": "WebPage",
  "name": "Vouch Mobile Plans & Pricing",
  "description": description,
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": site.url + "/plans/"
  },
  "mainEntity": {
    "@type": "ItemList",
    "itemListElement": [
      {
        "@type": "ListItem",
        "position": 1,
        "item": {
          "@type": "Product",
          "name": site.plans.premium.name + " Plan",
          "description": site.plans.premium.tagline,
          "brand": {
            "@type": "Brand",
            "name": site.name
          },
          "offers": {
            "@type": "AggregateOffer",
            "lowPrice": site.plans.premium.annualPrice,
            "highPrice": site.plans.premium.monthlyPrice,
            "priceCurrency": "USD",
            "availability": "https://schema.org/InStock",
            "priceValidUntil": "2026-12-31",
            "offerCount": 2
          },
          "audience": {
            "@type": "Audience",
            "audienceType": "Budget-conscious consumers seeking quality wireless service"
          }
        }
      },
      {
        "@type": "ListItem",
        "position": 2,
        "item": {
          "@type": "Product",
          "name": site.plans.elite.name + " Plan",
          "description": site.plans.elite.tagline,
          "brand": {
            "@type": "Brand",
            "name": site.name
          },
          "offers": {
            "@type": "AggregateOffer",
            "lowPrice": site.plans.elite.annualPrice,
            "highPrice": site.plans.elite.monthlyPrice,
            "priceCurrency": "USD",
            "availability": "https://schema.org/InStock",
            "priceValidUntil": "2026-12-31",
            "offerCount": 2
          },
          "audience": {
            "@type": "Audience",
            "audienceType": "Power users seeking premium features and priority data"
          }
        }
      }
    ]
  }
} | dump | safe }}
</script>
{% endblock %}

<!-- HERO: Bold pricing statement -->
<section class="plans-hero">
  <div class="plans-hero__bg">
    <div class="plans-hero__grid"></div>
    <div class="plans-hero__glow plans-hero__glow--1"></div>
    <div class="plans-hero__glow plans-hero__glow--2"></div>
    <div class="plans-hero__particles">
      {% for i in range(0, 20) %}
      <div class="plans-hero__particle"></div>
      {% endfor %}
    </div>
  </div>

  <div class="container">
    <div class="plans-hero__content">
      <div class="plans-hero__badge">
        <span class="plans-hero__badge-icon">
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M12 2L15.09 8.26L22 9.27L17 14.14L18.18 21.02L12 17.77L5.82 21.02L7 14.14L2 9.27L8.91 8.26L12 2Z"/>
          </svg>
        </span>
        No Credit Check • No Contracts
      </div>

      <h1 class="plans-hero__title">
        <span class="plans-hero__title-line">Unlimited Plans</span>
        <span class="plans-hero__title-accent">Starting at $25/mo</span>
      </h1>

      <p class="plans-hero__subtitle">
        Premium wireless on AT&T's nationwide network. Cancel anytime.
      </p>

      <div class="plans-hero__stats">
        <div class="plans-hero__stat">
          <span class="plans-hero__stat-value">$480</span>
          <span class="plans-hero__stat-label">Avg. yearly savings</span>
        </div>
        <div class="plans-hero__stat-divider"></div>
        <div class="plans-hero__stat">
          <span class="plans-hero__stat-value">97%</span>
          <span class="plans-hero__stat-label">US coverage</span>
        </div>
        <div class="plans-hero__stat-divider"></div>
        <div class="plans-hero__stat">
          <span class="plans-hero__stat-value">5G</span>
          <span class="plans-hero__stat-label">Network access</span>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- BILLING TOGGLE -->
<section class="plans-toggle-section">
  <div class="container">
    <div class="plans-toggle">
      <span class="plans-toggle__label" data-billing="monthly">Monthly</span>
      <label class="plans-toggle__switch">
        <input type="checkbox" id="billing-toggle" checked>
        <span class="plans-toggle__slider">
          <span class="plans-toggle__knob"></span>
        </span>
        <span class="sr-only">Toggle between monthly and annual billing</span>
      </label>
      <span class="plans-toggle__label plans-toggle__label--active" data-billing="annual">
        Annual
        <span class="plans-toggle__save">Save $60/yr</span>
      </span>
    </div>
  </div>
</section>

<!-- PRICING CARDS -->
<section class="plans-cards">
  <div class="container">
    <div class="plans-cards__grid">
      <!-- Premium Plan -->
      <div class="plan-card" data-plan="premium">
        <div class="plan-card__header">
          <div class="plan-card__icon">
            <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
              <path d="M3.75 13.5l10.5-11.25L12 10.5h8.25L9.75 21.75 12 13.5H3.75z"/>
            </svg>
          </div>
          <h2 class="plan-card__name">{{ site.plans.premium.name }}</h2>
          <p class="plan-card__tagline">{{ site.plans.premium.tagline }}</p>
        </div>

        <div class="plan-card__price">
          <span class="plan-card__currency">$</span>
          <span class="plan-card__amount" data-monthly="{{ site.plans.premium.monthlyPrice }}" data-annual="{{ site.plans.premium.annualPrice }}">{{ site.plans.premium.annualPrice }}</span>
          <span class="plan-card__period">/mo</span>
        </div>
        <p class="plan-card__billing annual-note">Billed ${{ site.plans.premium.annualTotal }}/year</p>
        <p class="plan-card__billing monthly-note" style="display: none;">Billed monthly</p>

        <ul class="plan-card__features">
          {% for feature in site.plans.premium.features %}
          <li class="plan-card__feature">
            <svg class="plan-card__check" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3">
              <path d="M5 13l4 4L19 7"/>
            </svg>
            {{ feature }}
          </li>
          {% endfor %}
        </ul>

        <a href="https://vouchmobile.com" class="plan-card__cta">
          Get Premium
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M5 12h14M12 5l7 7-7 7"/>
          </svg>
        </a>
      </div>

      <!-- Elite Plan -->
      <div class="plan-card plan-card--featured" data-plan="elite">
        <div class="plan-card__badge">Most Popular</div>
        <div class="plan-card__glow"></div>

        <div class="plan-card__header">
          <div class="plan-card__icon">
            <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
              <path d="M11.48 3.499a.562.562 0 011.04 0l2.125 5.111a.563.563 0 00.475.345l5.518.442c.499.04.701.663.321.988l-4.204 3.602a.563.563 0 00-.182.557l1.285 5.385a.562.562 0 01-.84.61l-4.725-2.885a.563.563 0 00-.586 0L6.982 20.54a.562.562 0 01-.84-.61l1.285-5.386a.562.562 0 00-.182-.557l-4.204-3.602a.563.563 0 01.321-.988l5.518-.442a.563.563 0 00.475-.345L11.48 3.5z"/>
            </svg>
          </div>
          <h2 class="plan-card__name">{{ site.plans.elite.name }}</h2>
          <p class="plan-card__tagline">{{ site.plans.elite.tagline }}</p>
        </div>

        <div class="plan-card__price">
          <span class="plan-card__currency">$</span>
          <span class="plan-card__amount" data-monthly="{{ site.plans.elite.monthlyPrice }}" data-annual="{{ site.plans.elite.annualPrice }}">{{ site.plans.elite.annualPrice }}</span>
          <span class="plan-card__period">/mo</span>
        </div>
        <p class="plan-card__billing annual-note">Billed ${{ site.plans.elite.annualTotal }}/year</p>
        <p class="plan-card__billing monthly-note" style="display: none;">Billed monthly</p>

        <ul class="plan-card__features">
          {% for feature in site.plans.elite.features %}
          <li class="plan-card__feature">
            <svg class="plan-card__check" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3">
              <path d="M5 13l4 4L19 7"/>
            </svg>
            {{ feature }}
          </li>
          {% endfor %}
        </ul>

        <a href="https://vouchmobile.com" class="plan-card__cta plan-card__cta--primary">
          Get Elite
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M5 12h14M12 5l7 7-7 7"/>
          </svg>
        </a>
      </div>
    </div>

    <p class="plans-cards__note">All plans include taxes & fees. Cancel anytime. 30-day money-back guarantee.</p>
  </div>
</section>

