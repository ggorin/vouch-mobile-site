<!-- HOW IT WORKS -->
<section class="home-steps">
  <div class="container">
    <div class="home-steps__header">
      <span class="home-steps__label">Getting Started</span>
      <h2 class="home-steps__title">Switch in 3 Easy Steps</h2>
    </div>

    <div class="home-steps__grid">
      <div class="home-steps__item">
        <div class="home-steps__number">01</div>
        <div class="home-steps__connector"></div>
        <div class="home-steps__content">
          <h3>Pick Your Plan</h3>
          <p>Choose Premium ($25/mo) or Elite ($35/mo). Both include unlimited everything.</p>
        </div>
      </div>

      <div class="home-steps__item">
        <div class="home-steps__number">02</div>
        <div class="home-steps__connector"></div>
        <div class="home-steps__content">
          <h3>Activate Instantly</h3>
          <p>Download eSIM in seconds or get a free physical SIM shipped to you.</p>
        </div>
      </div>

      <div class="home-steps__item">
        <div class="home-steps__number">03</div>
        <div class="home-steps__content">
          <h3>Start Saving</h3>
          <p>You're connected! Enjoy premium wireless and keep the extra $40+/month.</p>
        </div>
      </div>
    </div>

    <div class="home-steps__cta">
      <a href="/plans/" class="home-steps__btn">
        Get Started Now
        <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
          <path d="M5 12h14M12 5l7 7-7 7"/>
        </svg>
      </a>
    </div>
  </div>
</section>

<!-- PLANS PREVIEW -->
<section class="home-plans" id="pricing">
  <div class="container">
    <div class="home-plans__header">
      <span class="home-plans__label">Plans</span>
      <h2 class="home-plans__title">Simple, Transparent Pricing</h2>
      <p class="home-plans__subtitle">No hidden fees. All plans include taxes.</p>
    </div>

    <div class="home-plans__grid">
      {% for planKey, plan in site.plans %}
      <div class="home-plans__card{% if plan.popular %} home-plans__card--featured{% endif %}">
        {% if plan.popular %}<div class="home-plans__badge">Most Popular</div>{% endif %}

        <div class="home-plans__card-header">
          <h3 class="home-plans__name">{{ plan.name }}</h3>
          <p class="home-plans__tagline">{{ plan.tagline }}</p>
        </div>

        <div class="home-plans__price">
          <span class="home-plans__currency">$</span>
          <span class="home-plans__amount">{{ plan.annualPrice }}</span>
          <span class="home-plans__period">/mo</span>
        </div>
        <p class="home-plans__billing">with annual billing</p>

        <ul class="home-plans__features">
          {% for feature in plan.features | batch(5) | first %}
          <li>
            <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3">
              <path d="M5 13l4 4L19 7"/>
            </svg>
            {{ feature }}
          </li>
          {% endfor %}
        </ul>

        <a href="/plans/" class="home-plans__cta{% if plan.popular %} home-plans__cta--primary{% endif %}">
          Get {{ plan.name }}
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M5 12h14M12 5l7 7-7 7"/>
          </svg>
        </a>
      </div>
      {% endfor %}
    </div>

    <p class="home-plans__note">
      <a href="/plans/">View all plan details & features →</a>
    </p>
  </div>
</section>

<!-- COVERAGE PREVIEW -->
<section class="home-coverage" id="coverage">
  <div class="container">
    <div class="home-coverage__grid">
      <div class="home-coverage__content">
        <span class="home-coverage__label">Coverage</span>
        <h2 class="home-coverage__title">Nationwide 5G Coverage</h2>
        <p class="home-coverage__text">
          Vouch Mobile runs on AT&T's award-winning network—the same towers, the same speeds, just without the premium price tag.
        </p>

        <div class="home-coverage__stats">
          <div class="home-coverage__stat">
            <span class="home-coverage__stat-value">97%</span>
            <span class="home-coverage__stat-label">US Population Covered</span>
          </div>
          <div class="home-coverage__stat">
            <span class="home-coverage__stat-value">5G</span>
            <span class="home-coverage__stat-label">Nationwide Access</span>
          </div>
        </div>

        <a href="/coverage/" class="home-coverage__btn">
          Check Your Coverage
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M5 12h14M12 5l7 7-7 7"/>
          </svg>
        </a>
      </div>

      <div class="home-coverage__visual">
        <div class="home-coverage__map">
          {% image "/images/coverage-map.png", "US Coverage Map showing nationwide 5G network coverage", [400, 800, 1200], "(max-width: 768px) 100vw, 800px", {class: "home-coverage__map-img"} %}
        </div>
      </div>
    </div>
  </div>
</section>

<!-- COMPARE -->
<section class="home-compare">
  <div class="container">
    <div class="home-compare__header">
      <span class="home-compare__label">Compare</span>
      <h2 class="home-compare__title">See How We Stack Up</h2>
    </div>

    <div class="home-compare__table-wrap">
      <table class="home-compare__table">
        <thead>
          <tr>
            <th></th>
            <th class="home-compare__th-vouch">Vouch</th>
            <th>Verizon</th>
            <th>AT&T</th>
            <th>T-Mobile</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>Unlimited Plan</td>
            <td class="home-compare__td-vouch"><strong>$25/mo</strong></td>
            <td>$65+/mo</td>
            <td>$65+/mo</td>
            <td>$60+/mo</td>
          </tr>
          <tr>
            <td>Credit Check</td>
            <td class="home-compare__td-vouch">
              <span class="home-compare__badge home-compare__badge--yes">None</span>
            </td>
            <td><span class="home-compare__badge home-compare__badge--no">Required</span></td>
            <td><span class="home-compare__badge home-compare__badge--no">Required</span></td>
            <td><span class="home-compare__badge home-compare__badge--no">Required</span></td>
          </tr>
          <tr>
            <td>Contract</td>
            <td class="home-compare__td-vouch">
              <span class="home-compare__badge home-compare__badge--yes">None</span>
            </td>
            <td><span class="home-compare__badge home-compare__badge--no">Often</span></td>
            <td><span class="home-compare__badge home-compare__badge--no">Often</span></td>
            <td><span class="home-compare__badge home-compare__badge--no">Sometimes</span></td>
          </tr>
          <tr>
            <td>5G Access</td>
            <td class="home-compare__td-vouch">
              <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3"><path d="M5 13l4 4L19 7"/></svg>
            </td>
            <td><svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3"><path d="M5 13l4 4L19 7"/></svg></td>
            <td><svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3"><path d="M5 13l4 4L19 7"/></svg></td>
            <td><svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3"><path d="M5 13l4 4L19 7"/></svg></td>
          </tr>
        </tbody>
      </table>
    </div>

    <div class="home-compare__links">
      <span>Detailed comparisons:</span>
      {% for competitor in competitors | batch(3) | first %}
      <a href="/compare/vouch-vs-{{ competitor.slug }}/">vs {{ competitor.name }}</a>
      {% endfor %}
      <a href="/compare/">View all →</a>
    </div>
  </div>
</section>

<!-- FINAL CTA -->
<section class="home-cta">
  <div class="home-cta__bg">
    <div class="home-cta__grid"></div>
    <div class="home-cta__glow"></div>
  </div>

  <div class="container">
    <div class="home-cta__content">
      <h2 class="home-cta__title">Ready to Save $480+ Per Year?</h2>
      <p class="home-cta__text">
        Switch to Vouch Mobile in minutes. No credit check, no contracts, no surprises.
      </p>

      <div class="home-cta__buttons">
        <a href="/plans/" class="home-cta__btn home-cta__btn--primary">
          Get Started
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M5 12h14M12 5l7 7-7 7"/>
          </svg>
        </a>
        <a href="/coverage/" class="home-cta__btn home-cta__btn--secondary">
          Check Coverage
        </a>
      </div>

